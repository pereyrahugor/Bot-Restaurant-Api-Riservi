    { "key": "ASSISTANT_ID", "name": "Assistant ID", "required": true },
    { "key": "ASSISTANT_ID_IMG", "name": "Assistant ID Imagen", "required": false },
    { "key": "OPENAI_API_KEY", "name": "Clave API OpenAI", "required": true },
    { "key": "OPENAI_API_KEY_IMG", "name": "Clave API OpenAI Imagen", "required": false },
